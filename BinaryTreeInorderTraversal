/*
Given the root of the Binary tree, return the inorder traversal of its node's values

Input: root = [1,null,2,3]
Output: [1,3,2]

*/
class Solution{
  public:
   vector<int> answer;
   void inorder(TreeNode*root){
     if(root == NULL){
      return;
     }
     inorder(root->left);
     answer.push_back(root->val);
     inorder(root->right):
   }
   vector<int> inOrderTraversal(TreeNode*root){
     inorder(root);
     return answer;
   }
};
